<view class="icontainer">
    <!-- search bar and QR code scanner -->
    <view class="searchBarContainer">
        <input class="searchBar" placeholder="搜索标签数据" />
        <image class="scanQRCode" src='/images/scan1.png'></image>
    </view>
    <!-- list container -->
    <view class="itemList">
        <view class="card" bindtap="proTrace">
            <image class="imgs" src='/images/trace.png'></image>
            <view class="title">产品追溯</view>
        </view>
        <view class="card" bindtap="proRecord">
            <image class="imgs" src='/images/record.png'></image>
            <view class="title">鉴真记录</view>
        </view>
        <!-- <view class="card">
            <image class="imgs" src='/images/proInfo.png'></image>
            <view class="title">产品说明</view>
        </view> -->
        <view class="card" bindtap="proQualification">
            <image class="imgs" src='/images/qualification.png'></image>
            <view class="title">资质证书</view>
        </view>
    </view>
</view>