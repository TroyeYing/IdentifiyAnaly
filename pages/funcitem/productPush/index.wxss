.icontainer {
    background: #fff;
    color: rgb(95, 95, 95);
  }
  
  .top {
    width: 100%;
    margin-top: 20rpx;
  }
  
  .addBtn{
    width: 14vw;
    height: 14vw;
    border-radius: 100%;
    background-color: rgb(28, 125, 253);
    color: rgba(255, 255, 255);
    font-size: 60px;
    font-weight: 500;
    line-height: 30px;
    text-align: center;
    position: fixed;
    bottom: 36px;
    right: 36px;
  }

  .input {
    margin-left: 3%;
    width: 80%;
    height: 60rpx;
    border-radius: 30rpx;
    background-color: rgba(246, 247, 249, 1);
    padding-left: 5%;
    font-size: 26rpx;
  }
  
  .img {
    float: right;
    margin-top: -55rpx;
    margin-right: 25rpx;
    width: 50rpx;
    height: 50rpx;
  }
  
  .swiper-container {
    border-radius: 20rpx;
    position: relative;
    margin-left:3% ;
    width: 93%;
    margin-top:20rpx ;
  }
  
  .swiper-container .swiper .picture {
    border-radius: 20rpx;
    width: 100%;
    height: 100%;
  }
  
  
  .searchBarContainer {
    margin: 12px;
    padding: 4px 8px;
    border-radius: 10px;
    background-color: rgb(236, 236, 236);
    display: flex;
    align-items: center;
    justify-content: space-between;
  }
  
  .cardList {
    padding: 12px;
  }
  
  .card {
    border-radius: 8px;
    background-color: white;
    box-shadow: 0 0 8px rgb(177, 177, 177);
    margin-bottom: 16px;
  }
  
  .cardLabel {
    padding: 4px 8px;
  }
  
  .showDetail {
    border-top: 1px solid rgb(214, 214, 214);
    margin: 8px 0 4px 0;
  }